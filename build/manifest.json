{
    "version": "1.0.0",
    "manifest_version": 3,
    "name": "ClipBee",
    "description": "ClipBee: Universal Clipboard Chrome Extension",
    "permissions": [
        "clipboardRead",
        "clipboardWrite",
        "storage"
    ],
    "host_permissions": [
        "https://firestore.googleapis.com/",
        "https://www.gstatic.com/firebasejs/"
    ],
    "background": {
    },
    "action": {
        "default_popup": "index.html",
        "default_title": "ClipBee"
    }
}